<h1 class="logo">AlrightEros</h1>
<h1 class="welcome-message">A dating app clone</h1>

<form class="signup">
<h1>Sign Up</h1>

  <fieldset class="form-fieldset text-input-fieldset group">
    <label for="user-email">Email</label>
    <input
      type="text"
      class="text-input"
      name="user[email]"
      id="user-email"
      required>
  </fieldset>

  <fieldset class="form-fieldset text-input-fieldset group">
    <label for="user-password">Password</label>
    <input
      type="password"
      class="text-input"
      name="user[password]"
      id="user-password"
      required>
  </fieldset>

  <button class="submit">Sign Up</button>
</form>
