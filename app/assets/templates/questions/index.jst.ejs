<h1>All the questions</h1>
<nav class="page-buttons group">
  <button class='button page-button previous'>Prev Page</button>
  <button class='button page-button next'>Next Page</button>
</nav>
<a href="#/questions/new" class="button">Add a question</a>
<ul class="questions-list">
  <% questions.each(function(question){ %>
    <li>
      <a href="#/questions/<%= question.escape("id") %>" class="question-li">
        <%= question.escape("question_body") %>
      </a>
    </li>
  <% }) %>
</ul>
<nav class="page-buttons group">
  <button class='button page-button previous'>Prev Page</button>
  <button class='button page-button next'>Next Page</button>
</nav>
